{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "vite": ["node_modules/vite"]
    },
    "types": [
      "bun-types",
      "vinxi/client"
    ],
    "lib": [
      "esnext",
      "dom",
      "webworker"
    ],
    "preserveSymlinks": true,
    "sourceMap": true
  }
}
