{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"allowJs": true,
		"strict": true,
		"checkJs": true,
		"noEmit": false,
		"rootDir": "./",
		"isolatedModules": true,
		"declarationMap": true,
		"declaration": true,
		"emitDeclarationOnly": true,
		"outDir": "./dist/types",
		"paths": {
			"vinxi/*": ["./*"]
		}
	},
	"include": ["lib", "runtime", "types"]
}
